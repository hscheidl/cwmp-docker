config acs 'acs'
    option url 'http://host.docker.internal:8081/ACS'
    option userid 'admin'
    option passwd 'admin'
    option periodic_inform_enable '1'
    option periodic_inform_interval '90'
    option compression 'Disabled'
    option retry_min_wait_interval '5'
    option retry_interval_multiplier '2000'
    option insecure_enable '1'

config cpe 'cpe'
    option manufacturer 'icwmp'
    option manufacturer_oui 'FFFFFF'
    option product_class 'icwmp'
    option serial_number 'FFFFFF123456'
    option software_version '1.0'
    option model_name ''
    option description ''
    option log_to_console '1'
    option log_severity 'DEBUG'
    option userid 'admin'
    option passwd 'admin'
    option port '7547'
    option path '/'
    option provisioning_code ''
    option amd_version '6'
    option session_timeout '60'
    option notification '1'
    option exec_download '1'
    option periodic_notify_enable '1'
    option periodic_notify_interval '10'
#    list allowed_cr_ip '10.5.3.14/32'
#    list allowed_cr_ip '20.2.1.0/24'

config lwn 'lwn'
    option enable '1'
    option hostname 'http://host.docker.internal:8081/ACS'
